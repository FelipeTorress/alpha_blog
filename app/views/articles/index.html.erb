<h1>Página de listagem de artigos</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Descrição</th>
      <th>Ações</th>
    </tr>
  </thead>

  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td> <%= article.title %> </td>
        <td> <%= article.description %> </td>
        <td> PlaceHolder </td>
      </tr>
    <% end %>
  </tbody>
</table>